contract Remote =
  entrypoint id : ('a) => 'a
  entrypoint missing : ('a) => 'a
  entrypoint wrong_type : (string) => string

contract Main =

  entrypoint id(x : int) =
    x

  entrypoint wrong_type(x : int) =
    x

  entrypoint remote_id(r : Remote, x) =
    r.id(x)

  entrypoint remote_missing(r : Remote, x) =
    r.missing(x)

  entrypoint remote_wrong_type(r : Remote, x) =
    r.wrong_type(x)

